#include "main.h"
/**
 * *_strcat -concatenates two strings
 * @dest: string 1
 * @src: string 2
 * Return: concatenated string
 * */

char *_strcat(char *dest, char *src)
{
	int i, a;

	i = 0;
	a = 0;

	while (dest[1])
	{
		i++;
	}
	while (src[a])
	{
		dest[i] = src[a];
		i++;
		a++;
	}
	return (dest);
}
